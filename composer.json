{
    "name": "sfmbbrand/omnipay-gpn",
    "type": "library",
    "description": "GPN SDK library",
    "keywords": [
        "gpn",
        "2co",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "twocheckout"
    ],
    "homepage": "https://github.com/sfmbbrand/omnipay-gpn",
    "license": "MIT",
    "authors": [
        {
            "name": "Rolando Toledo",
            "email": "rtf@gpndata.com"
        },
        {
            "name": "Antonio Antelo Vazquez",
            "email": "aav@gpndata.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\GPNDataEurope\\" : "src/" }
    },
    "require": {
        "php": ">=8.0",
        "ext-bcmath": "*",
        "omnipay/common": "^v3.3.0",
        "symfony/http-client": "^6.0.20",
        "php-http/guzzle7-adapter": "^1.0.0",
      "ext-simplexml": "*"
    },
    "require-dev": {
        "omnipay/tests": "^4.1.2",
        "http-interop/http-factory-guzzle": "^1.2.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
